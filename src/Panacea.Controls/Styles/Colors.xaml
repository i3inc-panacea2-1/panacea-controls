<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:Panacea.Controls.Styles">
    <SolidColorBrush x:Key="ColorError" Color="#F44336" />
    <SolidColorBrush x:Key="ColorInformation" Color="#1fa8ef" />
    <SolidColorBrush x:Key="ColorWarning" Color="#ff7e28" />
    <SolidColorBrush x:Key="ColorNone" Color="LightGray" />
    <SolidColorBrush x:Key="ColorSuccess" Color="#00a31b" />
</ResourceDictionary>